clear all; close; clc;

counter = 0;
palendrome = [];
k_ = 0;
j_ = 0;

for k = 100:999
    for j = 100:999
        string_ = num2str(k*j);
        if string_(1) == string_(end)
            if string_(2) == string_(end-1)
                if string_(3) == string_(end-2)
                    palendrome.append(string_);
                    k_ = k
                    j_ = j
                end
            end
        end
    end
end

disp(palendrome)
disp("k is " + k_)
disp("j is " + j_)
